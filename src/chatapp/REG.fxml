<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFF9C4;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="chatapp.REGController">
   <children>
      <Button fx:id="yalla" layoutX="216.0" layoutY="257.0" mnemonicParsing="false" onAction="#login" style="-fx-background-color: #d2691e;" text="Yalla !!" textFill="WHITE" />
      <TextField fx:id="name" layoutX="88.0" layoutY="200.0" onAction="#login" prefHeight="31.0" prefWidth="319.0" promptText="Please Enter Your Name" />
      <Label layoutX="108.0" layoutY="74.0" text="JETS Chat Room" textFill="#f80b0b">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <VBox layoutX="451.0" layoutY="31.0" prefHeight="142.0" prefWidth="99.0">
         <children>
           <ImageView fx:id="profileImg" fitHeight="108.0" fitWidth="137.0" pickOnBounds="true" preserveRatio="true">
                <image>
                    <Image url="@addPhoto.jpg" />
                </image>
            </ImageView>
            <Button fx:id="choosePic" mnemonicParsing="false" onAction="#uploadPic" prefHeight="25.0" prefWidth="138.0" style="-fx-background-color: #d2691e;" text="Choose Pic" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
